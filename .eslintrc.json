{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "extends": ["airbnb-base", "eslint:recommended"],
    "rules": {
        "quotes": ["error", "single"],
        "semi": ["warn", "always"],
        "comma-dangle": ["off"],
        "indent": ["off"],
        "no-console": ["off"],
        "prefer-arrow-callback": ["warn"],
        "eol-last": ["off"]
    }
}